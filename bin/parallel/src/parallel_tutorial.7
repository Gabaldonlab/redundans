\&  #!/usr/bin/parallel \-\-shebang\-wrap /usr/bin/ruby
\&  
\&  print "Arguments "
\&  puts ARGV
.Ve
.IP "Octave:" 9
.IX Item "Octave:"
.Vb 1
\&  #!/usr/bin/parallel \-\-shebang\-wrap /usr/bin/octave
\&  
\&  printf ("Arguments");
\&  arg_list = argv ();
\&  for i = 1:nargin
\&    printf (" %s", arg_list{i});
\&  endfor
\&  printf ("\en");
.Ve
.IP "Common \s-1LISP:\s0" 9
.IX Item "Common LISP:"
.Vb 1
\&  #!/usr/bin/parallel \-\-shebang\-wrap /usr/bin/clisp
\&  
\&  (format t "~&~S~&" \*(AqArguments)
\&  (format t "~&~S~&" *args*)
.Ve
.IP "\s-1PHP:\s0" 9
.IX Item "PHP:"
